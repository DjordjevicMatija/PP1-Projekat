package rs.ac.bg.etf.pp1;

public class sym {

	// KEYWORDS
	public static final int PROG = 1;    // "program"
	public static final int BREAK = 2;   // "break"
	public static final int CLASS = 3;   // "class"
	public static final int ELSE = 4;    // "else"
	public static final int CONST = 5;   // "const"
	public static final int IF = 6;      // "if"
	public static final int NEW = 7;     // "new"
	public static final int PRINT = 8;   // "print"
	public static final int READ = 9;    // "read"
	public static final int RETURN = 10; // "return"
	public static final int VOID = 11;   // "void"
	public static final int EXTENDS = 12;// "extends"
	public static final int CONTINUE = 13;// "continue"
	public static final int FOR = 14;    // "for"
	public static final int STATIC = 15; // "static"

	// IDENTIFIERS
	public static final int IDENT = 16;  // ident = letter {letter | digit | '_'}

	// CONSTANTS
	public static final int NUM_CONST = 17;  // numConst = digit {digit}
	public static final int CHAR_CONST = 18; // charConst = '\'' printableChar '\''
	public static final int BOOL_CONST = 19; // boolConst = ("true" | "false")

	// OPERATORS
	public static final int PLUS = 20;       // "+"
	public static final int MINUS = 21;      // "-"
	public static final int MULTIPLY = 22;   // "*"
	public static final int DIVIDE = 23;     // "/"
	public static final int MODULO = 24;     // "%"
	public static final int EQUALS = 25;     // "=="
	public static final int NOT_EQUALS = 26; // "!="
	public static final int GREATER_THAN = 27; // ">"
	public static final int GREATER_EQUAL = 28; // ">="
	public static final int LESS_THAN = 29;  // "<"
	public static final int LESS_EQUAL = 30; // "<="
	public static final int AND = 31;        // "&&"
	public static final int OR = 32;         // "||"
	public static final int ASSIGN = 33;     // "="
	public static final int INCREMENT = 34;  // "++"
	public static final int DECREMENT = 35;  // "--"
	public static final int SEMICOLON = 36;  // ";"
	public static final int COLON = 37;      // ":"
	public static final int COMMA = 38;      // ","
	public static final int DOT = 39;        // "."
	public static final int LPAR = 40;       // "("
	public static final int RPAR = 41;       // ")"
	public static final int LBRACKET = 42;   // "["
	public static final int RBRACKET = 43;   // "]"
	public static final int LBRACE = 44;     // "{"
	public static final int RBRACE = 45;     // "}"
	public static final int ARROW = 46;      // "=>"

	// END OF FILE
	public static final int EOF = 47;

}
